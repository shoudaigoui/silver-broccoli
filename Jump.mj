Jump(addr:[26]) = new_pc:[32] where
	x = add<32>(pc, Extension_de_signe<28>(0100));
	new_pc = x[28..31] . addr . 00
end where

main(addr:[26]) = new_pc:[32] where
	new_pc = Jump(addr)
end where
